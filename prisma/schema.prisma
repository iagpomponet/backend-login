generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id       String @unique
  username String
  email    String @unique
  password String
}

model product {
  id          String   @unique
  description String
  name        String
  price       Int
  created_at  DateTime
}
