generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id       String @unique
  username String
  email    String @unique
  password String
}

model product {
  id          String   @unique
  description String
  name        String
  price       Int
  created_at  DateTime
}
